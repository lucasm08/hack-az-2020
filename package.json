    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "customize-cra": "^0.9.1",
    "react-app-rewired": "^2.1.5",
    "wasm-loader": "^1.3.0"
  }
}
